@using Traveltrip.Models.Sınıflar
@model List<Blog>
<div class="box1-left">
    @foreach(var x in Model)
     { 
    <div class="bot-img">
        <img src="@x.BlogImage" style="width:160px; height:90px;" alt="" />
    </div>
    <div class="bot-desc1">
        <p>@x.Acıklama.Substring(0,70) </p>
    </div>

    <div class="clear"></div>
        }
</div>
